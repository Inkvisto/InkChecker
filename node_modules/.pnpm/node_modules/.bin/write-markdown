#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/ts-command-line-args@2.5.0_jest@28.1.3_typescript@5.0.4/node_modules/ts-command-line-args/dist/node_modules:/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/ts-command-line-args@2.5.0_jest@28.1.3_typescript@5.0.4/node_modules/ts-command-line-args/node_modules:/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/ts-command-line-args@2.5.0_jest@28.1.3_typescript@5.0.4/node_modules:/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/ts-command-line-args@2.5.0_jest@28.1.3_typescript@5.0.4/node_modules/ts-command-line-args/dist/node_modules:/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/ts-command-line-args@2.5.0_jest@28.1.3_typescript@5.0.4/node_modules/ts-command-line-args/node_modules:/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/ts-command-line-args@2.5.0_jest@28.1.3_typescript@5.0.4/node_modules:/home/inkvisto/Desktop/programming/InkChecker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-command-line-args/dist/write-markdown.js" "$@"
else
  exec node  "$basedir/../ts-command-line-args/dist/write-markdown.js" "$@"
fi
